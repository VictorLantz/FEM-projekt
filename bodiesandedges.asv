%Tar reda på vilka kroppar som är vilka och vilka edges som är vilka i vår
%mesh.

%Skapar vektorer med materialkonstanter. Mappar rÃ¤tt konstant till rÃ¤tt
%kropp
k = [0 0 0 0];
c = [0 0 0 0];
ro = [0 0 0 0];

%% Hittar SMD-kroppen:
i = 1;
while ~(point(1,i) == 0 && point(2,i) == 6*10^-4)   
    i = i+1;
end
col = 1;
while ~sum(find(triangle(1:3, col) == i))
    col = col+1;
end
k(triangle(4,c)) = ksmd;
c(triangle(4,c)) = csmd;
ro(triangle(4,c)) = rosmd;


%% Hittar PCB-kroppen:
i = 1;
while ~(point(1,i) == 1*10^-3 && point(2,i) == 0)   
    i = i+1;
end
col = 1;
while ~sum(find(triangle(1:3, col) == i))
    col = col+1;
end
k(triangle(4,c)) = kpcb;
c(triangle(4,c)) = cpcb;
ro(triangle(4,c)) = rpcb;
% k = [ksol kpcb ksmd];
% c = [csol cpcb csmd];
% ro = [rsol rpcb rosmd];


%% Hittar Solder-kroppen
i = 1;
while ~(point(1,i) == 1*10^-3 && point(2,i) == 2 * 10^-4)   
    i = i+1;
end
col = 1;
while ~sum(find(triangle(1:3, col) == i && k(triangle(4, col)) == 0))
    col = col+1;
end
k(triangle(4,c)) = ksol;
c(triangle(4,c)) = cpcb;
ro(triangle(4,c)) = rpcb;
